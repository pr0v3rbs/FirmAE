+            command_handler(args)
+        except RoutersploitException as err:
+            print_error(err)
+        except EOFError:
+            print_info()
+            print_status("routersploit stopped")
+            return
+        except KeyboardInterrupt:
+            print_info()
+        finally:
+            printer_queue.join()
